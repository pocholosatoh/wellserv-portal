#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pocholo/Projects/wellserv-portal/node_modules/.pnpm/react-native@0.81.4_@babel+core@7.28.5_@types+react@19.2.6_react@19.1.0/node_modules/react-native/node_modules:/Users/pocholo/Projects/wellserv-portal/node_modules/.pnpm/react-native@0.81.4_@babel+core@7.28.5_@types+react@19.2.6_react@19.1.0/node_modules:/Users/pocholo/Projects/wellserv-portal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pocholo/Projects/wellserv-portal/node_modules/.pnpm/react-native@0.81.4_@babel+core@7.28.5_@types+react@19.2.6_react@19.1.0/node_modules/react-native/node_modules:/Users/pocholo/Projects/wellserv-portal/node_modules/.pnpm/react-native@0.81.4_@babel+core@7.28.5_@types+react@19.2.6_react@19.1.0/node_modules:/Users/pocholo/Projects/wellserv-portal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-native/cli.js" "$@"
else
  exec node  "$basedir/../react-native/cli.js" "$@"
fi
