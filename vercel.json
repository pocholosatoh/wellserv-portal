{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "pnpm install",
  "buildCommand": "pnpm --filter web build",
  "outputDirectory": "apps/web/.next",
  "framework": "nextjs",
  "functions": {
    "api/**": {
      "maxDuration": 60
    }
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- apps/web"
}
